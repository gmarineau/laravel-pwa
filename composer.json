{
    "name": "gmarineau/laravelpwa",
    "description": "Looks like an app, feels like an app, but NOT an app.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "php",
        "pwa",
        "progressive web apps"
    ],
    "authors": [
        {
            "name": "Greg Marineau",
            "email": "greg.marineau@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/http": "^8.0|^9.0|^10.0",
        "illuminate/view": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "GMarineau\\LaravelPwa\\": "src"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "GMarineau\\LaravelPwa\\Providers\\LaravelPwaServiceProvider"
            ]
        }
    }
}
